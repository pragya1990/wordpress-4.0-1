#!/bin/sh
LD_LIBRARY_PATH="/Applications/wordpress-4.0-1/common/lib:$LD_LIBRARY_PATH"
DYLD_LIBRARY_PATH="/Applications/wordpress-4.0-1/common/lib:$DYLD_LIBRARY_PATH"
CURL_CA_BUNDLE="/Applications/wordpress-4.0-1/common/openssl/certs/curl-ca-bundle.crt"
export LD_LIBRARY_PATH
export DYLD_LIBRARY_PATH
export CURL_CA_BUNDLE
exec /Applications/wordpress-4.0-1/common/bin/curl.bin "$@"
